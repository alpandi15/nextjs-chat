{
  "presets": ["next/babel"],
  "plugins": [
    ["styled-components", { "ssr": true }],
    [
      "module-resolver",
      {
        "root": [
          "./"
        ],
        "alias": {
          "services": "./services",
          "constants": "./constants",
          "components": "./components",
          "pages": "./pages",
          "static": ".",
          "public": "./public",
          "styles": "./styles"
        }
      }
    ]
  ]
}